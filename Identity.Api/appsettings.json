{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "DbmOptions": {
        "MaxRetries": 5,
        "RetryDelayMilliseconds": 1000,
        "MaxConcurrentStatements": 20,
